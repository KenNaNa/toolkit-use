{
  "name": "toolkit-use",
  "version": "0.0.15",
  "description": "a npm toolkit use for vue.js and react",
  "main": "dist/index.umd.js",
  "types": "types/index.d.ts",
  "module": "dist/index.esm.js",
  "keywords": ["npm", "js", "ts", "node", "library", "typescript", "toolkit", "element-ui", "element-plus", "vue3", "vue", "vuejs", "uikit", "react", "use"],
  "scripts": {
    "serve": "rollup -c -w", 
    "build": "rimraf dist/ && rollup -c",
    "babel": "babel ./src/index.js -o ./dist/index.js",
    "test": "jest",
    "release": "release-it",
    "release:beta": "release-it major --preRelease=beta",
    "fix:src": "npx eslint src --fix --ext .ts",
    "fix:test": "npx eslint test --fix --ext .js",
    "lint": "npm run fix:src && npm run fix:test"
  },
  "author": "kennana",
  "license": "MIT",
  "repository": "https://github.com/KenNaNa/toolkit-use.git",
  "homepage": "https://kennana.github.io/toolkit-use",
  "devDependencies": {
    "@babel/cli": "^7.22.6",
    "@babel/core": "^7.22.6",
    "@babel/plugin-transform-runtime": "^7.22.7",
    "@babel/preset-env": "^7.22.6",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.0",
    "babel-jest": "27.0.2",
    "jest": "27.0.2",
    "jest-globals": "^0.1.7",
    "release-it": "^16.0.0",
    "rollup": "^3.26.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-jest": "27.0.2",
    "tslib": "^2.6.0",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.6",
    "clipboard": "^2.0.11",
    "core-js": "^3.31.1",
    "rimraf": "^4"
  }
}
